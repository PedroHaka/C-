#include <stdio.h>
#include <stdlib.h>

int a;

int main ()
{
    system("color 0b");
    printf("  Multiplos de 3 entre 0 e 100");

a=0;

while (a<=100)

       {
           a++;
           if (a % 3 == 0)
           {
              printf("\n %d", a);

           }
       }
    printf("\n");
    system ("PAUSE\n");
}
