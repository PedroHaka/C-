#include <stdio.h>
#include <stdlib.h>
int main()
{
      int num, i;
      i = 0;
      printf("Digite um numero para mostrar a tabuada deste numero. \n");
      scanf("%d", &num);
        do
        {
           i++;
          printf("\n %4d X %2d = %4d", num, i, i * num);
        }
        while (i<10);
        getchar();
        return(0);
}
